Proceso Area_triangulo
	Definir _base Como Real;
	Definir _altura Como Real;
	Definir _area Como Real;
	Escribir "Ingrese la base del triangulo: ";
	Escribir "Ingrese la altura del triangulo: ";
	Leer _base;
	Leer _altura;
	_area <- (_base*_altura)/2;
	Escribir "El área del triángulo "," es: ", _area;
	
FinProceso
